


<h2>愿望清单</h2>
<div class="container">
    <table class="table">
      
        @foreach (var item in ViewBag.Data)
        {

        <tr><td style="border:1px solid grey;border-radius:3px"><a href="/Helper/List/@item">@item</a></td></tr>

        }
    </table>
</div>
<form class="form-inline">
    
    <input type="text" class="form-control" name="id" id="id" placeholder="添加新愿望单" />
    <button type="submit" class="btn btn-default">提交</button>

</form>
